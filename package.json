{
  "name": "@fnxsdev/express-sensible",
  "version": "1.0.0",
  "description": "Mountable Express.js middlewares with sensible defaults and async context management",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "express",
    "middleware",
    "async-context",
    "cors",
    "etag",
    "rate-limiting",
    "body-parser",
    "security",
    "performance"
  ],
  "author": "FNXS Dev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hharzer/express-sensible.git"
  },
  "peerDependencies": {
    "express": "^4.18.0 || ^5.0.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}